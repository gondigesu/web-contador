 border-radius:10px;\r\n    box-shadow: 2px 2px 2px grey;\r\n    font-family: 'Roboto', sans-serif;\r\n}\r\n\r\n.modalservicios, .modalrs, .modalbio{\r\n    display: none;\r\n    position: fixed;\r\n    z-index: 1; \r\n    left: 0;\r\n    top: 0;\r\n    width: 100%; \r\n    height: 100%;\r\n    overflow: auto; \r\n    background-color: rgb(0,0,0);\r\n    background-color: rgba(0,0,0,0.4); \r\n}\r\n\r\n.modal-contenido {\r\n    background-color:white;\r\n    margin: 5% auto; \r\n    padding: 20px;\r\n    border: 1px solid #888;\r\n    width: 50%; \r\n    border-radius:30px;\r\n  }\r\n\r\n  .closebio, .closers, .closeservicios {\r\n    color: #aaa;\r\n    float: right;\r\n    font-size: 28px;\r\n    font-weight: bold;\r\n  }\r\n\r\n  .closers:hover,\r\n.closers:focus,\r\n.closeservicios:focus,\r\n.closeservicios:hover,\r\n.closebio:hover,\r\n.closebio:focus {\r\n  color: black;\r\n  text-decoration: none;\r\n  cursor: pointer;\r\n}\r\n\r\n.linea{\r\n    background:black;\r\n    height:3px;\r\n    width:100%;\r\n}\r\n\r\n.redes li,\r\n.servicios li{\r\n    font-size:20px;\r\n    font-family: 'Roboto', sans-serif;\r\n    padding:15px;\r\n    text-align: center;\r\n}\r\n\r\n\r\n.redes, .servicios {\r\n    display:grid;\r\n    grid-template-columns: repeat(auto-fit, minmax(175px, 1fr));\r\n    list-style:none;\r\n    grid-gap:20px;\r\n    padding:20px;\r\n    align-items:center;\r\n}\r\n\r\n.redes{\r\n    grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));\r\n}\r\n\r\n.textobio{\r\n    padding:25px;\r\n    margin:10px;\r\n    font-family: 'Roboto', sans-serif;\r\n    text-align: justify;\r\n    font-size:15px;\r\n}\r\n\r\n@media only screen and (max-width: 768px) {\r\n\r\n    .contenedor{\r\n        width:100vw;\r\n        height:170vh;\r\n    }\r\n\r\n.titulo{\r\n    font-size:25px;\r\n    padding:10px;\r\n    padding-bottom:10px;\r\n}\r\n\r\n.opciones{\r\n    display:grid;\r\n    grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));\r\n    color:white;\r